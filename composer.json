{
    "name": "ntzm/strict-casts",
    "type": "library",
    "description": "Strict type casting for super defensive PHP",
    "license": "MIT",
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "phpbench/phpbench": "^0.16.9",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "StrictCasts\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "StrictCastsBenchmarks\\": "benchmarks/",
            "StrictCastsTests\\": "tests/"
        }
    }
}
